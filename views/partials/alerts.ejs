<% if (alerts.error) { %>
  <% alerts.error.forEach(function(msg) { %>
    <div class="alert-banner alert-danger"><%= msg %></div>
  <% }); %>
<% } %>

<% if (alerts.success) { %>
  <% alerts.success.forEach(function(msg) { %>
    <div class="alert-banner alert-success"><%= msg %></div>
  <% }); %>
<% } %>
